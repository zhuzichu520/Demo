<?xml version="1.0" encoding="utf-8"?>
<layout>

    <data>

        <variable
            name="item"
            type="com.netease.nim.demo.ui.message.emoticon.viewmodel.ItemViewModelEmoji" />
    </data>

    <com.hiwitech.android.shared.widget.SquareRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:onClickCommand="@{item.onClickEmojiCommand}"
        android:background="@drawable/nim_emoji_item_selector">

        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_centerInParent="true"
            app:srcDrawable="@{item.darawble}"
            tools:src="@mipmap/ic_launcher" />

    </com.hiwitech.android.shared.widget.SquareRelativeLayout>
</layout>